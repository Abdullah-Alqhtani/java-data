			int r1 = n1 + n2;	
			
			System.out.println(r1);
			
				  if (r1 > 50 && r1<=100  ) {
			
			System.out.println("you have passed");}
			
			else if (r1 < 50) {
			 
				System.out.println("you have failed");
			}
			else {
